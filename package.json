{
  "name": "WeTest Extension",
  "version": "0.0.1",
  "lockfileVersion": 3,
  "requires": true,
  "activationEvents": [
    "onCommand:generateUnitTest"
  ],
  "description": "A VS Code extension to generate unit tests using the LLMs.",
  "displayName": "Test Generator",
  "main": "./dist/extension.js",
  "packages": {},
  "contributes": {
    "configuration": {
        "type": "object",
        "title": "WeTest Extension Settings",
        "properties": {
            "WeTest.llmApis": {
                "type": "object",
                "default": {
                    "gemini": "",
                    "chatgpt": ""
                },
                "description": "API keys for different LLMs. Provide keys for the ones you use."
            }
        }
    },
    "commands": [
      {
        "command": "generateUnitTestFolder",
        "title": "WeTest: Generate Unit Test For Folder"
      },
      {
        "command": "generateUnitTestFile",
        "title": "WeTest: Generate Unit Test For File"
      },
      {
        "command": "generateUnitTestSelected",
        "title": "WeTest: Generate Unit Test For Selected Code"
      },
      {
        "command": "configureApis",
        "title": "Configure LLM APIs"
      }
    ]
  },
  "devDependencies": {
    "@types/node": "^16.18.3",
    "@types/vscode": "^1.95.0",
    "nyc": "^17.1.0",
    "typescript": "^4.9.5",
    "vscode": "^1.1.37"
  },
  "engines": {
    "vscode": "^1.70.0"
  },
  "scripts": {
    "compile": "tsc -p ./",
    "vscode:prepublish": "tsc -p ./"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "axios": "^1.7.7",
    "dotenv": "^16.4.5",
    "openai": "^4.67.3"
  }
}
