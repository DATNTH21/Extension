{
  "name": "Extension",
  "version": "0.0.1",
  "lockfileVersion": 3,
  "requires": true,
  "activationEvents": [
    "onCommand:gemini.generateUnitTest"
  ],
  "description": "A VS Code extension to generate unit tests using the Gemini API.",
  "displayName": "Gemini Test Generator",
  "main": "./dist/extension.js",
  "packages": {},
  "contributes": {
    "commands": [
      {
        "command": "generateUnitTestFolder",
        "title": "WeTest: Generate Unit Test For Folder"
      },
      {
        "command": "generateUnitTestFile",
        "title": "WeTest: Generate Unit Test For File"
      },
      {
        "command": "generateUnitTestSelected",
        "title": "WeTest: Generate Unit Test For Selected Code"
      },
      {
        "command": "resetAPI",
        "title": "WeTest: Reset API"
      }
    ]
  },
  "devDependencies": {
    "@types/node": "^16.18.3",
    "@types/vscode": "^1.95.0",
    "typescript": "^4.9.5",
    "vscode": "^1.1.37"
  },
  "engines": {
    "vscode": "^1.70.0"
  },
  "scripts": {
    "compile": "tsc -p ./",
    "vscode:prepublish": "tsc -p ./"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "axios": "^1.7.7",
    "dotenv": "^16.4.5",
    "openai": "^4.67.3"
  }
}
